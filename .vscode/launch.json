{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "F-Match",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cv_server/run_matching_server.py",
            "args": [
                "--type=super_point",
                "--save_dir=${workspaceFolder}/debug/Match",
                "--query_dir=${workspaceFolder}/debug/Scene",
                "--train_dir=${workspaceFolder}/debug/SFM",
                "--port=9090"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "S-Match",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/super_point/run_superglue_server.py",
            "args": [
                "--type=super_point",
                "--save_dir=${workspaceFolder}/debug/Match",
                "--query_dir=${workspaceFolder}/debug/Scene",
                "--train_dir=${workspaceFolder}/debug/SFM",
                "--port=9090"
            ],
            "cwd": "${workspaceFolder}/super_point/",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "View Model",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/model_viewer.py",
            "args": [
                "--model_dir",
                "${workspaceFolder}/data/cracker_box/cracker_box_1/star",
                "--eval_dir",
                "${workspaceFolder}/data/cracker_box/cracker_box_4",
                "--sparse_model_path",
                "${workspaceFolder}/data/sfm_model/cracker_box/outputs_superpoint_superglue/tkl_model"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debug Render",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/debug_render.py",
            "args": [
                "--model_path",
                "${workspaceFolder}/data/cracker_box/cracker_box_1/star",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Shift Model",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/model_shift.py",
            "args": [
                "--data_dir",
                "${workspaceFolder}/data/cracker_box/cracker_box_1",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Record RL",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/camera_utils.py",
            "args": [
                "--output_dir",
                "${workspaceFolder}/data/jelly_box/jelly_box_2",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Transfer [op]",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/star_to_one_pose.py",
            "args": [
                "--data_dir",
                "${workspaceFolder}/data/cracker_box/cracker_box_1"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Transfer [icg]",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/star_to_icg.py",
            "args": [
                "--tracker_name",
                "cracker_box",
                "--model_dir",
                "${workspaceFolder}/data/cracker_box/cracker_box_1",
                "--eval_dir",
                "${workspaceFolder}/data/cracker_box/cracker_box_4",
                "--icg_dir",
                "${workspaceFolder}/ICG/examples/generator_example",
                "--detector_port=8080",
                "--feature_port=9090",
                "--enable_feature",
                "--enable_depth",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Transfer [rbot]",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/rbot_to_icg.py",
            "args": [
                "--tracker_name",
                "bakingsoda",
                "--model_dir",
                "${workspaceFolder}/data/RBOT_dataset/bakingsoda",
                "--eval_dir",
                "${workspaceFolder}/data/RBOT_dataset/bakingsoda",
                "--icg_dir",
                "${workspaceFolder}/ICG/examples/generator_example_v2",
                "--detector_port=8080",
                "--feature_port=9090",
                "--enable_feature",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "OnePose: [sfm]",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/one_pose_sfm.py",
            "args": [
                "+preprocess=sfm_spp_spg_custom.yaml",
                "redo=True"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "OnePose: [inf]",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/one_pose_inference.py",
            "args": [
                "+experiment=test_GATsSPG_custom.yaml",
                "object_detect_mode=GT_box",
                "save_wis3d=True"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "OnePose: [prior]",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/one_pose_prior.py",
            "args": [
                "+experiment=one_pose_prior.yaml",
                "input.eval_dirs=[${workspaceFolder}/data/cracker_box/cracker_box_4]",
                "video_mode=image"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "OnePose: [server]",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/one_pose_server.py",
            "args": [
                "+experiment=one_pose_server.yaml",
                "port=8080"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "ICG",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/examples/run_generated_tracker",
            "args": [
                "${workspaceFolder}/ICG/examples/generator_example_v2/config.yaml"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "r2d2 (server)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/r2d2/run_server.py",
            "args": [
                "--port=9090",
                "--model=models/r2d2_WASF_N16.pt",
                "--top-k=500"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/r2d2"
        },
        {
            "name": "super_point (server)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/super_point/run_superpoint_server.py",
            "args": [
                "--port=9090",
                "--top-k=500"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/super_point"
        }
    ]
}